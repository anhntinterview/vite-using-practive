{
  "name": "using-practice",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev:server": "nodemon --watch server.ts --watch src/server --exec 'ts-node server-dev.ts'",
    "dev:client": "yarn build:client && vite --config vite.config.ts dev",
    "build": "rimraf dist && tsc -p tsconfig.prod.json && yarn build:client && yarn build:server && yarn copy-files",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir dist/server",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "serve": "yarn build && cross-env NODE_ENV=production yarn ts-node ./server-dev",
    "serve:local": "vite serve",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles static/* dist/assets && copyfiles index.html dist && copyfiles -f dist/client/assets/* dist/assets",
    "format": "prettier --write ."
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "nodemon": "^3.0.1",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "serve-static": "^1.15.0",
    "ts-node": "^10.9.1",
    "url": "^0.11.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.17",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/serve-static": "^1.15.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.15",
    "copyfiles": "^2.4.1",
    "cssnano": "^6.0.1",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.29",
    "postcss-custom-properties": "^13.3.0",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-plugin": "^1.0.0",
    "postcss-preset-env": "^9.1.3",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "sass": "^1.66.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.4"
  }
}
